{
  "name": "hello-world",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "parcel build index.ts --target node --out-dir parcel --bundle-node-modules",
    "lint": "eslint --fix *.ts",
    "test": "cd .. && bst test",
    "posttest": "jest-coverage-badges input '../test_output/coverage/coverage-summary.json' '../badges'",
    "predeploy": "npm run lint && npm run test && npm run build && cd .. && ask validate -l en-US",
    "deploy": "NODE_ENV=production cd .. && ask deploy"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ask-sdk-core": "^2.6.0",
    "ask-sdk-dynamodb-persistence-adapter": "^2.6.0",
    "ask-sdk-model": "^1.17.0",
    "i18next": "^17.0.4",
    "install": "^0.12.2",
    "npm": "^6.9.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run lint --",
      "npm run test --",
      "ask validate",
      "git add"
    ],
    "*.json": [
      "npm run test --",
      "ask validate",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^8.2.1",
    "typescript": "^3.5.2"
  }
}
